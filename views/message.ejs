  <div class="container" id="chatForm">

    <div class="main-container users">
      <div class="profile-container">
        <div class="profile-user photo">
          <div class="profile-user-image">
            <img src="./images/user.png">
          </div>
        </div>

        <div class="profile-user heading">
          <p id="myUser"><b><%=user %></b></p>
        </div>
        <div class="profile-user people">
          <div class="profile-user-people">
            <div class="profile-setting-image">
              <h3 id="peoples">5</h3>
            </div>
          </div>
        </div>
        <div class="profile-user setting">
          <div class="profile-user-setting">
            <div class="profile-setting-image">
              <img src="./images/setting.jfif">
            </div>
          </div>
        </div>
        <div class="profile-user edit">
          <div class="profile-user-edit">
            <div class="profile-edit-image" id="create-room">
              <img src="./images/edit.png">
            </div>
          </div>
        </div>

        <div class="setting-model">
          <div class="setting-model-setting">
            <a>
              <p>Settings</p>
            </a>
          </div>
          <div class="setting-model-active">
            <a>
              <p>Active Contacts</p>
            </a>
            <a>
              <p>Message Requests</p>
            </a>
            <a>
              <p>Archived Chats</p>
            </a>
            <a>
              <p>Unread Chats</p>
            </a>
          </div>
          <div class="setting-model-about">
            <a>
              <p>About</p>
            </a>
            <a>
              <p>Terms</p>
            </a>
            <a>
              <p>Privacy policy</p>
            </a>
            <a>
              <p>Cookies policy</p>
            </a>
          </div>
          <div class="setting-model-help">
            <a>
              <p>Help</p>
            </a>
            <a>
              <p>Report a problem</p>
            </a>
          </div>
          <div class="setting-model-logout">
            <a>
              <p>Log out</p>
            </a>
          </div>
        </div>
      </div>
      <div class="user-container">
        <div class="search-container">
          <div class="search-container-inside">
            <div class="search-user button">
              <button><i class="fas fa-search"></i></button>
            </div>
            <div class="search-user text">
              <input type="text" name="users" placeholder="search messenger..">
            </div>
          </div>
        </div>
        <ul class="user-main-container" id="listUser">
          <li class="user-main-container-inside" id="A-All">
            <div class="user-main-inside photo">
              <div class="user-main-image">
                <img src="./images/users.jpg">
              </div>
            </div>
            <div class="user-main-inside text">
              <div class="user-main-name">
                <div class="user-main-name-title">
                  <p>Chat All</p>
                </div>
                <div class="user-main-status" id="status">
                  <!-- <span class="dot"></span> -->
                </div>
              </div>
              <div class="user-main-message">
                <div class="user-last-message message">
                  <p id="last-mes-all">Last seen message</p>
                </div>
                <div class="user-last-message date">
                  <p>Date</p>
                </div>
              </div>
            </div>
          </li>
        </ul>
      </div>
    </div>

    <div class="main-container chat">
      <div class="main-navbar">
        <div class="main-navbar-inside photo">
          <div class="navbar-image">
            <img src="./images/users.jpg">
          </div>
        </div>
        <div class="main-navbar-inside name">
          <div class="navbar-name">
            <p id="userVictim">All</p>
          </div>
          <div class="navbar-time">
            <p>Time</p>
          </div>
        </div>
        <div class="main-navbar-inside icon">
          <div class="navbar-icon">
            <img src="./images/call.png">
          </div>
        </div>
        <div class="main-navbar-inside icon">
          <div class="navbar-icon">
            <img src="./images/video_call.png">
          </div>
        </div>
        <div class="main-navbar-inside icon">
          <div class="navbar-icon">
            <img src="./images/information.png">
          </div>
        </div>
      </div>
      <div class="user-chat-container">
        <div class="user-chat-main-container chat">
          <div class="chat-chat-container" id="chat-container">
            <!-- <div class="chat-right">
                            <div class="chat-right-page image">
                                <div class="chat-display-image">
                                    <img src="./images/check.jfif">
                                </div>
                            </div>
                            <div class="chat-right-page name">
                                <div class="chat-display-name">
                                    <p>Hello</p>
                                </div>
                            </div>

                        </div>
                        <div class="chat-left">
                            <div class="chat-left-page image">
                                <div class="chat-left-display-image">
                                    <img src="./images/users.jpg">
                                </div>
                            </div>
                            <div class="chat-left-page name">
                                <div class="chat-left-display-name">
                                    <p>How are you?</p>
                                </div>
                            </div>
                        </div> -->
          </div>
          <div class="chat-bottom">
            <div class="chat-bottom-icon icon">
              <div class="chat-bottom-image">
                <img src="./images/add.jfif">
              </div>
            </div>
            <div class="chat-bottom-icon icon">
              <div class="chat-bottom-image">
                <img src="./images/gif.png">
              </div>
            </div>
            <div class="chat-bottom-icon icon">
              <div class="chat-bottom-image">
                <img src="./images/sticker.jfif">
              </div>
            </div>
            <div class="chat-bottom-icon icon">
              <div class="chat-bottom-image">
                <img src="./images/file.png">
              </div>
            </div>
            <div class="chat-bottom-icon send">
              <div class="chat-bottom-send">
                <div class="chat-bottom-input text" id="input-send-chat">
                  <input type="text" name="Send" placeholder="Type a message.." id="send-all">
                </div>
                <div class="chat-bottom-input smile">
                  <div class="chat-bottom-image-smile">
                    <img src="./images/smile.png">
                  </div>
                </div>
              </div>
            </div>
            <div class="chat-bottom-icon icon">
              <div class="chat-bottom-like-image">
                <img src="./images/fire.png">
              </div>
            </div>
          </div>
        </div>
        <div class="user-chat-main-container profile">
          <div class="user-profile-detail">
            <div class="user-profile-image">
              <img src="./images/users.jpg">
            </div>
            <div class="user-profile-name" id="user-profile-name">
              <!-- <p><b>Username</b></p> -->
            </div>
            <div class="user-profile-time">
              <p>Time</p>
            </div>
          </div>
          <div class="option-container">
            <div class="option-heading">
              <div class="option-name name">
                <p>OPTIONS</p>
              </div>
              <div class="option-name icon">
                <div class="option-heading-image">
                  <img src="./images/arrowdown.png">
                </div>
              </div>
            </div>
            <div class="option-search">
              <div class="option-search-name name">
                <p>Search in Conversation</p>
              </div>
              <div class="option-search-name icon">
                <div class="option-search-image">
                  <img src="./images/search.png">
                </div>
              </div>
            </div>
            <div class="option-edit">
              <div class="option-edit-name name">
                <p>Edit Nickname</p>
              </div>
              <div class="option-edit-name icon">
                <div class="option-edit-image">
                  <img src="./images/editNickname.png">
                </div>
              </div>
            </div>
            <div class="option-color">
              <div class="option-color-name name">
                <p>Change Color</p>
              </div>
              <div class="option-color-name icon">
                <div class="option-color-image">
                  <img src="./images/color.png">
                </div>
              </div>
            </div>
            <div class="option-emoji">
              <div class="option-emoji-name name">
                <p>Change Emoji</p>
              </div>
              <div class="option-emoji-name icon">
                <div class="option-emoji-image">
                  <img src="./images/like.png">
                </div>
              </div>
            </div>
           <div class="input-room">
              <input type="text" id="txtCreateRoom">
              <input  type="button" id="btnCreateRoom" value="Tao room">
           </div>
            <div class="option-logOut" id="logOut">
              <form action="/logout" method="POST">
                
                  <input value="LogOut" type="submit" id="logOut"class="rainbow rainbow-2"></input>
                
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script>
  </script>
</body>

</html>